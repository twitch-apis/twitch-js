{
  "name": "twitch-js",
  "version": "1.2.5",
  "description": "Javascript library for the Twitch Messaging Interface.",
  "license": "MIT",
  "author": "Schmoopiie",
  "keywords": [
    "tmi",
    "twitch",
    "twitch.tv",
    "stream",
    "broadcast",
    "message",
    "messaging",
    "interface",
    "subscriber",
    "websocket",
    "ws",
    "bot",
    "robot"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/marcandrews/twitch-js.git"
  },
  "bugs": {
    "url": "https://github.com/marcandrews/twitch-js/issues"
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "tonicExampleFilename": "examples/runkit.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "lint": "eslint src",
    "test": "mocha --compilers js:babel-core/register --reporter spec --require should test",
    "test:coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run build",
    "prebuild": "npm run clean",
    "build:cjs": "babel src --out-dir lib",
    "build:umd:dev": "webpack",
    "build:umd:prod": "cross-env NODE_ENV=PRODUCTION webpack",
    "build": "run-p build:**"
  },
  "dependencies": {
    "request": "2.74.0",
    "ws": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "8.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.6.0",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.1",
    "eslint": "4.13.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "hook-std": "0.2.0",
    "mocha": "2.2.5",
    "npm-run-all": "4.1.2",
    "nyc": "^11.3.0",
    "rimraf": "2.5.2",
    "should": "7.0.4",
    "webpack": "3.10.0"
  },
  "browser": {
    "ws": false,
    "request": false
  }
}
